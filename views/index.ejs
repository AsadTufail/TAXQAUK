        const stripe = Stripe('pk_test_51N1WPyElVjnShDCotmL7DbI6Llh0E69NeQnOmumyLpp000sSbVLWrf67qPuVHbXLJWBlh7fzoED8kUS3RODOWOGO00NgFHw2y7'); // Your Publishable Key
        const elements = stripe.elements();

        // Create our card inputs
        var style = {
            base: {
                color: "black"
            }
        };

        const card = elements.create('card', { style });
        card.mount('#card-element');

        const form = document.getElementById("paymentForm");
        const errorEl = document.querySelector('#card-errors');


        // Create token from card data
        form.addEventListener('submit', e => {
            e.preventDefault();
            stripe.createToken(card).then(res => {
                if (res.error) {
                    errorEl.textContent = res.error.message;
                } else {
                    const hiddenInput = document.createElement('input');
                    hiddenInput.setAttribute('type', 'hidden');
                    hiddenInput.setAttribute('name', 'stripeToken');
                    hiddenInput.setAttribute('value', res.token.id);
                    form.appendChild(hiddenInput);
                    form.submit();
                }
            })
        });
    </script>
